{
  "name": "shopping-list-app",
  "version": "0.1.0",
  "private": true,
  "proxy": "https://localhost:44306/",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "cypress": "cross-env REACT_APP_USE_MOCK_REPOS='true' && cypress open",
    "cypress:e2e": "concurrently \"npm run start\" \"cypress open\"",
    "cypress:batch:component": "cross-env REACT_APP_USE_MOCK_REPOS='true' && cypress run --component",
    "storybook": "npm run sass && storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:deploy": "storybook-to-ghpages",
    "sass": "sass ./src/styles/styles.scss ./public/main.css",
    "sass-watch": "sass --watch ./src/styles/styles.scss ./public/main.css"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/cypress": "^1.1.3",
    "date-fns": "^3.1.0",
    "nanoid": "^5.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.69.6",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "web-vitals": "^3.5.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/addon-styling-webpack": "^0.0.6",
    "@storybook/blocks": "^7.6.7",
    "@storybook/jest": "^0.2.3",
    "@storybook/preset-create-react-app": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-webpack5": "^7.6.7",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/test": "^7.6.7",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/bootstrap": "^5.2.10",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.68",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.1",
    "eslint-plugin-storybook": "^0.6.15",
    "prop-types": "^15.8.1",
    "storybook": "^7.6.7",
    "storybook-addon-state": "^1.0.3",
    "webpack": "^5.89.0"
  },
  "overrides": {
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
